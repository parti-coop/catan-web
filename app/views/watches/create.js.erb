<% if params[:partial] == 'intro' %>
  <% script = escape_javascript(render customize_partial_path(@watch.watchable, ->(model) { "#{model.class.model_name.plural}/card_in_intro" }), issue: @issue, campaign: @campaign) %>
  $('.<%= dom_id(@watch.watchable) %>-card').replaceWith(parti_origin_partial("<%= script %>"));
  <% unless current_user.need_to_more_watch? %>
    $('#btn-continue').removeClass( "disabled" );
    $('#btn-continue').attr("href", "<% root_path %>");
  <% end %>
<% else %>
  $('.<%= dom_id(@watch.watchable) %>-watch').html(parti_origin_partial("<%= escape_javascript(render 'watches/watch', issue: @issue, campaign: @campaign) %>"));
<% end %>



