<%
  member = message.messagable
  joinable = member.joinable
  body = "#{message.sender.nickname}님이 #{joinable.title} #{joinable.model_name.human}에 가입했습니다."
%>
{
  "notification": {
    "title": "<%= joinable.title %>",
    "body": "<%= body %>"
  },
  "data": {
    "title": "<%= joinable.title %>",
    "body": "<%= body %>",
    "type": "<%= joinable.is_a? Group ? 'group' : 'parti' %>",
    "param": "<%= joinable.slug %>"
  }
}
