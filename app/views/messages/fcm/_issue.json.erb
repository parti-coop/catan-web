<%
  issue = message.messagable
  body = "#{message.sender.nickname}님이 #{message.action_params_hash["previous_title"]} 빠띠 이름을 #{issue.title} 빠띠로 수정했습니다"
%>
{
  "notification": {
    "title": "<%= issue.title %>",
    "body": "<%= body %>"
  },
  "data": {
    "title": "<%= issue.title %>",
    "body": "<%= body %>",
    "type": "parti",
    "param": "<%= issue.slug %>"
  }
}

