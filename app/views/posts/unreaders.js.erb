<% if @previous_last.blank? and params[:last_id].present? %>
  alert("탈퇴한 사용자가 있습니다. 화면을 다시 새로 고쳐 보세요");
  $('.user-card-wrapper').data('is-last', true);
<% else %>
  $('.user-card-wrapper').data('last-id', '<%= @members.last.try(:id) %>');
  $('.user-card-wrapper').data('is-last', <%= @is_last_page %>);
  var $users = $("<%= escape_javascript(render 'members/members_page', issue: @issue, users: @users) %>");
  $('.user-card-wrapper').append($users);
  parti_partial$($users);
<% end %>
