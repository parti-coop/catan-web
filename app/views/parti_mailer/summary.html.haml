.email-container
  .email-header
    %a{href: 'http://parti.xyz'}
      %img{src: 'data:image/png;base64,', style: 'max-width: 100%;'}
  .email-content.text-center
    %p 안녕하세요. #{@user.nickname}님!
    %p #{Date.yesterday} Parti 유쾌한 민주주의 소식입니다!
    %p &nbsp;
    %ul#summary
    - @created_posts.each do |post|
      %li{style: 'text-align: left; list-style:none'}
        %a{href: url_for(post.specific), style: 'color: '}
          [#{post.specific.class.model_name.human}] #{post.specific.title}
        - if post.specific.respond_to?(:body)
          %p= truncate(strip_tags(post.specific.body), length: 100)
    %ul#summary
    - @hottest_posts.each do |post|
      %li{style: 'text-align: left; list-style:none'}
        %a{href: url_for(post.specific), style: 'color: '}
          [#{post.specific.class.model_name.human}] #{post.specific.title}
        - if post.specific.respond_to?(:body)
          %p= truncate(strip_tags(post.specific.body), length: 100)

  .email-footer
    %div{:style => "text-align: left; position: relative; margin: auto; width: 600px;"}
      %div{:style => "position: absolute; bottom:0; right: 300px;line-height: 20px;font-size: 90%;"}
        %a{:href => "http://parti.xyz", :style => "color: #999; text-decoration: none"} parti.xyz
        %br/
        %a{:href => "mailto:help@parti.xyz", :style => "color: #999; text-decoration: none"} help@parti.xyz
      %div{:style => "position: absolute; bottom:0; right: 65px;line-height: 20px;color: #999;font-size: 90%;"}
        %a{:href => "http://fb.com/parti.xyz", :style => "color: #999; text-decoration: none"} fb.com/parti.xyz
        %br/
        %a{:href => "http://medium.com/parti-xyz-developers", :style => "color: #999; text-decoration: none"} medium.com/parti-xyz-developers
      %div{style: 'margin-left: 30px;'}
        %a{href: 'http://parti.xyz'}
          %img{src: 'data:image/png;base64,', style: 'max-height: 40px;'}
