<% if @error %>
  UnobtrusiveFlash.showFlashMessage("<%= t('errors.messages.unknown') %>", {type: 'error'});

  var $message = $("<div style='text-decoration: underline;' class='only-clickable'>다시 저장하기</div>").on('click', function() {
    console.log($('.js-dragable-slug-folder'));
    $('.js-dragable-slug-folder').trigger('parti-dragable-slug-folder-item-save');
  });
  $('.js-slug-folder-autosave').html($message);
<% else %>
  $('.js-dragable-slug-folder').trigger('parti-dragable-slug-folder-item-destroy');
  var $dom = $('<%= j(render 'issues/slug_partial_folders', issue: @issue, folders: @folders, dom_id: params[:dom_id]) %>');
  $('#<%= params[:dom_id] %>').replaceWith($dom);
  parti_partial$($dom, true);

  var $message = $('<div>저장했습니다. <%= j(date_f(DateTime.now)) %></div>');
  $('.js-slug-folder-autosave').html($message);
  $('.js-slug-folder-autosave').find('time[data-time-ago]').timeago();
<% end %>
